    <ItemGroup>
      <PackageReference Include="Microsoft.Windows.CsWinRT" Version="2.2.0" />
      <PackageReference Include="Microsoft.Windows.SDK.Contracts" Version="10.0.26100.1742" />
    </ItemGroup>

</Project>
